            <enumeration value="EC" id="Electronic_Users" />
            <enumeration value="PR" id="Producer" />
            <enumeration value="RE1" id="Rebate_User_Schedule5" />
            <enumeration value="IDZ" id="Zone_Operator" />
            <enumeration value="CCA" id="Customs_Control_Area" />
            <enumeration value="WH2" id="Customs_Warehouse" />
            <enumeration value="CA" id="Clearing_Agent" />
            <enumeration value="RH" id="Road_Haulier" />
            <enumeration value="WH3" id="Warehouse_CCA_Storage" />
            <enumeration value="WH4" id="Warehouse_CCA_Manufacturing" />
            <enumeration value="CDP" id="Depot_Container" />
            <enumeration value="DDP" id="Depot_Degrouping" />
            <enumeration value="SHP" id="Ship_Wreck" />
            <enumeration value="COP" id="Container_Operator" />
